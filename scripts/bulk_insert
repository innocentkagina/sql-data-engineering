TRUNCATE TABLE  #truncate table
COPY Table_Name (columns) #Table to receive data
FROM '/path/to/employees.csv' # file path
DELIMITER ',' #delimitier
CSV HEADER; # source file type

### Example
TRUNCATE TABLE crm_cust_info
COPY crm_cust_info
FROM 'D:\share\datasets\source_crm\cust_info.csv'
DELIMITER ','
CSV HEADER; 
